{
  "name": "karaokemugen-app",
  "version": "3.0.1",
  "description": "Karaoke playlist manager / player",
  "bin": {
    "karaokemugen": "./dist/index.js"
  },
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts --files tsconfig.json",
    "frontend-dev": "cd frontend && yarn dev",
    "frontend-prod": "cd frontend && yarn build",
    "debug": "ts-node $NODE_DEBUG_OPTION src/index.ts -- --debug",
    "qstart": "node dist/index.js",
    "test": "mocha --require ts-node/register --timeout 20000 test/*.ts",
    "build": "npx rimraf \"dist\" && tsc --build",
    "setup": "git submodule sync --recursive && git submodule update --init --recursive && yarn install && yarn build && yarn installFrontend && yarn frontend-prod && yarn installSystemPanel && yarn buildSystemPanel",
    "gitconfig": "git config diff.submodule log && git config fetch.recursesubmodules on-demand && git config status.submodulesummary true && git config push.recursesubmodules on-demand && git config submodule.recurse true",
    "pull": "git pull && git submodule sync --recursive && git submodule update --init --recursive",
    "installFrontend": "cd frontend && yarn install",
    "installSystemPanel": "cd systempanel && yarn install",
    "buildSystemPanel": "cd systempanel && yarn build",
    "startSystemPanel": "cd systempanel && yarn start",
    "migrate": "node node_modules/db-migrate/bin/db-migrate sync [all]",
    "migrateDown": "node node_modules/db-migrate/bin/db-migrate down",
    "migrateUp": "node node_modules/db-migrate/bin/db-migrate up",
    "dropDatabase": "ts-node util/dropDatabase.ts"
  },
  "homepage": "https://karaokes.moe",
  "repository": {
    "type": "git",
    "url": "https://lab.shelter.moe/karaokemugen/karaokemugen-app.git"
  },
  "keywords": [
    "karaoke",
    "playlist",
    "mpv",
    "ffmpeg",
    "webapp"
  ],
  "bugs": {
