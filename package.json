{
  "name": "karaokemugen-app",
  "version": "2.5.0",
  "description": "Karaoke playlist manager / player",
  "bin": {
    "karaokemugen": "./dist/index.js"
  },
  "main": "src/index.js",
  "scripts": {
    "start": "babel-node src/index.js --",
    "debug": "babel-node $NODE_DEBUG_OPTION src/index.js -- --debug",
    "test": "mocha --require babel-register --timeout 20000",
    "build": "yarn buildReact && rm -Rf dist && babel --source-maps --copy-files -d dist/ src/",
    "install": "cd react_systempanel && yarn install",
    "setup": "yarn install && yarn buildReact",
    "buildReact": "cd react_systempanel && yarn build",
    "startReact": "cd react_systempanel && yarn start",
    "migrate": "node node_modules/db-migrate/bin/db-migrate sync [all] --migrations-dir src/_dao/migrations",
    "migrateDown": "node node_modules/db-migrate/bin/db-migrate down --migrations-dir src/_dao/migrations",
    "migrateUp": "node node_modules/db-migrate/bin/db-migrate up --migrations-dir src/_dao/migrations",
    "dropDatabase": "node util/dropDatabase.js"
  },
  "homepage": "https://mugen.karaokes.moe/en",
  "repository": {
    "type": "git",
    "url": "https://lab.shelter.moe/karaokemugen/karaokemugen-app.git"
  },
  "keywords": [
    "karaoke",
    "playlist",
    "mpv",
    "ffmpeg",
    "webapp"
  ],
  "bugs": {
    "url": "https://lab.shelter.moe/karaokemugen/karaokemugen-app/issues",
    "email": "axel@teri-chan.net"
  },
  "author": "Guillaume Lebigot <axel@teri-chan.net> (http://www.meido-rando.net)",
  "contributors": [
    "Mathieu Delestre <ldbglobe@gmail.com>",
