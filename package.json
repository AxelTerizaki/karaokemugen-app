{
  "name": "karaokemugen-app",
  "version": "3.0.0",
  "description": "Karaoke playlist manager / player",
  "bin": {
    "karaokemugen": "./dist/index.js"
  },
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts --files tsconfig.json",
	"debug": "ts-node $NODE_DEBUG_OPTION src/index.ts -- --debug",
	"qstart": "node dist/index.js",
    "test": "mocha --require ts-node/register --timeout 20000 test/*.ts",
    "build": "rm -Rf dist && tsc --build",
    "setup": "yarn install && yarn build && cd react_systempanel && yarn install && yarn build",
    "buildReact": "cd react_systempanel && yarn build",
    "startReact": "cd react_systempanel && yarn start",
    "migrate": "node node_modules/db-migrate/bin/db-migrate sync [all]",
    "migrateDown": "node node_modules/db-migrate/bin/db-migrate down",
    "migrateUp": "node node_modules/db-migrate/bin/db-migrate up",
    "dropDatabase": "ts-node util/dropDatabase.ts"
  },
  "homepage": "https://karaokes.moe",
  "repository": {
    "type": "git",
    "url": "https://lab.shelter.moe/karaokemugen/karaokemugen-app.git"
