            <div class="tab-content" id="nav-tabContent">
                <div id="nav-login" role="tabpanel" aria-labelledby="nav-login-tab" class="modal-body tab-pane fade in active">
                    <div class="modal-message">
                        <button class="btn btn-default guest">
                             {{i18n "GUEST_CONTINUE"}}
                        </button>
                    </div>
                    <div>
                     {{i18n "OR"}}
                    </div>
                    <div class="modal-message">
                            <input type="text" class="form-control" id="login" name="modalLogin" placeholder="{{i18n "NICKNAME"}}" required autofocus/>
